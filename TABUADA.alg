algoritmo "semnome"
// Função :
// Autor :
// Data : 10/01/2022
// Seção de Declarações 
var

   contador: inteiro
   tabuada: inteiro

inicio

      contador <- 0
      Escreva ("Tabuada do ")
      Leia (tabuada)

      Enquanto contador <= (tabuada*10) faca
               EscrevaL (contador)
               contador <- contador + tabuada
      FimEnquanto

fimalgoritmo
