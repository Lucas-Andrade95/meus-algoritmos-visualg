algoritmo "Creuza01"
// Função :
// Autor :
// Data : 04/01/2022
// Seção de Declarações 
var
   anoatual, anonascimento, idade: real

inicio

      Escreva ("Em que ano estamos? ")
      Leia (anoatual)
      Escreva ("Em que ano a Creuza nasceu? ")
      Leia (anonascimento)
      idade <- anoatual - anonascimento
      Escreva ("Então a Creuza tem ", idade, " anos!")
      
fimalgoritmo
