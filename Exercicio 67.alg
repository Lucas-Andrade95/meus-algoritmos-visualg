algoritmo "Exercicio 67"
// Função :
// Autor :
// Data : 21/01/2022
// Seção de Declarações 
var
     C, N: Inteiro
inicio

      Escreva ("Digite um número: ")
      Leia (N)
      
      Para C <- 0 ate N faca
      Escreva (C,",")
      C <- C + 1
      FimPara
      Escreva (" FIM!")

fimalgoritmo
