algoritmo "semnome"
// Função :
// Autor :
// Data : 10/01/2022
// Seção de Declarações 
var
   contador: inteiro
   x: inteiro
inicio

      contador <- 0
      Escreva ("Conte até ")
      Leia (x)
      Enquanto contador <= (x) faca
               EscrevaL (contador)
               contador <- contador + 1
      FimEnquanto
      
fimalgoritmo
